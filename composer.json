{
    "name": "dimer47/zero",
    "description": "Lightweight Docker wrapper for Laravel Zero CLI applications, inspired by Laravel Sail",
    "keywords": ["laravel-zero", "docker", "cli"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dimer47/zero/issues",
        "source": "https://github.com/dimer47/zero"
    },
    "authors": [
        {
            "name": "Dimitri Iachi",
            "email": "iachi.dimitri@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/console": "^9.52.16|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.52.16|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.52.16|^10.0|^11.0|^12.0",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^2.0"
    },
    "bin": [
        "bin/zero"
    ],
    "autoload": {
        "psr-4": {
            "Dimer47\\Zero\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dimer47\\Zero\\ZeroServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
